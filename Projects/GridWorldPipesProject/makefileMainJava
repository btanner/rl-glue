.PHONY: handle2 main stats clean

#------------------------
MAIN = main.java
#------------------------

HANDLE = pipes_main.java

all: handle2 stats main 

main: $(MAIN)
	javac -d Build/ -classpath Stats/:../../RL-Glue1p1/Pipes/  $(MAIN)

handle2: ../../RL-Glue1p1/Pipes/$(HANDLE)
	javac -d Build/ -classpath Build/ ../../RL-Glue1p1/Pipes/$(HANDLE)

stats: Stats/RL_util.java
	javac -d Build/ -classpath ../../RL-Glue1p1/Pipes/ Stats/RL_util.java
clean:
	-rm -rf *.class Build/*.class core* 

