.PHONY: handle agent spec clean

#------------------------
AGENT = SarsaAgent.java
#------------------------

HANDLE = agent_pipe_handler.java

all: spec agent handle

agent: $(AGENT)
	javac  -classpath Build -d Build/  $(AGENT)

handle: ../../RL-Glue1p1/Pipes/$(HANDLE)
	javac  -d Build/ ../../RL-Glue1p1/Pipes/$(HANDLE)

spec: Utils/Java_agent_parser.java
	javac  -d Build/ Utils/Java_agent_parser.java

clean:
	-rm -rf *.class Build/*.class core* 

